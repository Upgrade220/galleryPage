{
  "name": "galleryPage",
  "scripts": {
    "start": "better-npm-run start",
    "debug": "better-npm-run debug"
  },
  "betterScripts": {
    "debug": {
      "command": "node --experimental-modules index.mjs",
      "env": {
        "DEBUG": "express:*"
      }
    },
    "start": {
      "command": "nodemon --experimental-modules index.mjs",
      "env": {
        "DEBUG": "express:*"
      }
    }
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "nodemon": "^2.0.7"
  },
  "devDependencies": {
    "better-npm-run": "^0.1.1",
    "prettier": "^2.2.1"
  }
}
